    [1] = "On",
    [2] = haki
}

workspace.UserData["User_"..x].III:FireServer(unpack(args))
wait()
local args = {
    [1] = "Off",
    [2] = haki
}

workspace.UserData["User_"..x].III:FireServer(unpack(args))
wait()
local args = {
    [1] = "On",
    [2] = haki
}

workspace.UserData["User_"..x].III:FireServer(unpack(args))
wait()
local args = {
    [1] = "Off",
    [2] = haki
}

workspace.UserData["User_"..x].III:FireServer(unpack(args))
wait()
local args = {
    [1] = "On",
    [2] = haki
}

workspace.UserData["User_"..x].III:FireServer(unpack(args))
wait()
local args = {
    [1] = "Off",
    [2] = haki
}

